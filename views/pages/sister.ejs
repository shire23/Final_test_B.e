<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.16/dist/tailwind.min.css" rel="stylesheet" />
    <title>Aplikasi Pendaftaran Siswa - Siswa</title>
  </head>

  <body class="bg-gray-100">
    <!-- Navbar -->
    <nav class="bg-blue-500 py-4">
      <div class="container mx-auto">
        <a href="/" class="text-white text-lg font-semibold">Aplikasi Registrasi Siswa</a>
      </div>
    </nav>

    <!-- Content -->
    <div class="container mx-auto mt-8">
      <h1 class="text-2xl font-semibold mb-4"></h1>

      <div class="container">
        <!-- Header -->
        <header class="flex justify-between items-center mb-8">
          <h1 class="text-2xl font-bold"></h1>
          <a href="#" class="text-blue-500"></a>
        </header>

        <div id="login" class="mb-8">
          <h1 class="text-2xl font-bold mb-4">Login</h1>
          <form>
            <div class="mb-4">
              <label for="nama" class="block">Nama:</label>
              <input type="text" id="nama" name="nama" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nama" class="block">tanggal lahir:</label>
              <input type="text" id="tanggallahir" name="tanggallahir" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nama" class="block">tempat lahir:</label>
              <input type="text" id="tempat lahir" name="tempat lahir" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nomorhp" class="block">Kelas:</label>
              <input type="text" id="nomorhp" name="nomorhp" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nama" class="block">Alamat:</label>
              <input type="text" id="nomorhportu" name="nomorhportu" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nama" class="block">Nomor hp:</label>
              <input type="text" id="nomorhportu" name="nomorhportu" class="border rounded py-2 px-3 w-full" />
            </div>

            <div class="mb-4">
              <label for="nama" class="block">Nomor Hp Ortu:</label>
              <input type="text" id="nomorhportu" name="nomorhportu" class="border rounded py-2 px-3 w-full" />
            </div>

            <button type="submit" class="bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded">Login</button>

            <button type="submit" class="bg-blue-500 hover:bg-blue-600 text-white py-2 px-4 rounded">Logout</button>
          </form>
        </div>

        <!-- Tabel Daftar Siswa -->
        <table class="w-full bg-white border border-gray-300">
          <thead>
            <tr>
              <th class="py-2 px-4 border-b">Nama</th>
              <th class="py-2 px-4 border-b">Tanggal Lahir</th>
              <th class="py-2 px-4 border-b">tempat Lahir</th>
              <th class="py-2 px-4 border-b">Kelas</th>
              <th class="py-2 px-4 border-b">Alamat</th>
              <th class="py-2 px-4 border-b">Nomor HP</th>
              <th class="py-2 px-4 border-b">Nama Orang Tua</th>
              <th class="py-2 px-4 border-b">Nomor HP Orang Tua</th>
            </tr>
          </thead>
          <tbody>
            <!-- data siswa -->
            <tr>
